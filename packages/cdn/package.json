{
  "name": "@laughingwhales/appkit-cdn",
  "version": "1.8.12",
  "sideEffects": false,
  "main": "dist/wagmi.js",
  "type": "module",
  "files": [
    "dist",
    "!tsconfig.tsbuildinfo",
    "README.md"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build": "tsc && vite build",
    "watch": "tsc --watch",
    "upload": "pnpm build && pnpm publish --public",
    "serve": "http-server dist -p 8080 --cors"
  },
  "devDependencies": {
    "http-server": "14.1.1",
    "vite-plugin-node-polyfills": "0.23.0"
  },
  "peerDependencies": {
    "typescript": "5.9.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@wagmi/connectors": "5.11.2",
    "@wagmi/core": "2.21.2",
    "viem": "2.37.9",
    "vite": "5.4.20",
    "wagmi": "2.17.5",
    "@laughingwhales/appkit": "workspace:*",
    "@laughingwhales/appkit-adapter-ethers": "workspace:*",
    "@laughingwhales/appkit-adapter-ethers5": "workspace:*",
    "@laughingwhales/appkit-adapter-solana": "workspace:*",
    "@laughingwhales/appkit-adapter-wagmi": "workspace:*",
    "@laughingwhales/appkit-polyfills": "workspace:*"
  },
  "keywords": [
    "appkit",
    "wallet",
    "onboarding",
    "reown",
    "dapps",
    "web3",
    "wagmi",
    "ethereum",
    "solana",
    "bitcoin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reown-com/appkit.git"
  },
  "publishConfig": {
    "provenance": true
  }
}
